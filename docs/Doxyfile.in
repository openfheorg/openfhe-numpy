PROJECT_NAME           = "OpenFHE-Numpy"
PROJECT_NUMBER         = @OPENFHE_NUMPY_VERSION@
OUTPUT_DIRECTORY       = @CMAKE_CURRENT_BINARY_DIR@/docs/cpp
INPUT                  = @CMAKE_CURRENT_SOURCE_DIR@/core/include @CMAKE_CURRENT_SOURCE_DIR@/core/src
FILE_PATTERNS          = *.cpp *.h
RECURSIVE              = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
GENERATE_XML           = YES
XML_OUTPUT             = xml
LAYOUT_FILE            = @CMAKE_CURRENT_SOURCE_DIR@/docs/DoxygenLayout.xml
USE_MDFILE_AS_MAINPAGE = @CMAKE_CURRENT_SOURCE_DIR@/README.md